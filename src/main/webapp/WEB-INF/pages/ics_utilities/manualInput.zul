<panel id="panelVisit"
		title="${c:l('utilities.manualInput.panel.Visit')}"
		style="margin-bottom:10px" border="normal">
<panelchildren>
	<groupbox mold="3d" width="99%">
					<grid fixedLayout="true">
					<columns>
					<column width="300px" />
					<column width="100%" />
				</columns>
						<rows>
							<row>
								<label value="${c:l('utilities.manualInput.label.visitFlag')}" />
								<radiogroup id="rbVisit">
									<radio id="rVisitYes" label="YES"
										value="Y" checked="true"/>
									<radio id="rVisitNo"
										label="NO" value="N" />
								</radiogroup>
							</row>
							<row>
								<label value="${c:l('utilities.manualInput.label.visitNotes')}" />
								<textbox id="visitNotes" width="90%" maxlength="250" multiline="true" height="100px" />
							</row>
							<row>
								<label id="lblTrip" value="${c:l('utilities.manualInput.label.tripLength')}" />
								<decimalbox id="tripLength" width="300px" maxlength="50" />
							</row>
							<row visible="false">
								<label id="lblCount" value="${c:l('utilities.manualInput.label.countKPI')}" />
								<radiogroup id="rbCount">
									<radio id="rCountYes" label="YES"
										value="Y" checked="true" />
									<radio id="rCountNo" label="NO" 
										value="N" />
								</radiogroup>
							</row>		
						</rows>
					</grid>
					</groupbox>
	</panelchildren>
</panel>		


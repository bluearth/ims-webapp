							    	<hbox>
							    		<textbox id="tbCanvasserName" width="275px"/>
							    		<button id="btnCanvasserNameSearch" label="SEARCH"/>
							    	</hbox>						       
							        <listbox id="canvasserNameList" width="348px">
							        <listhead>
								        <listheader label="Id" width="10%" />

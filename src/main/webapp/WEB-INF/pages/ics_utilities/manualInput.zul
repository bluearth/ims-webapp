									    <listheader label="Type" width="25%"/>
							        </listhead>
							        </listbox>							        
							        <paging id="canvasserNamePaging" pageSize="20"/>
							    </bandpopup>							 
							</bandbox>
							<!--<combobox sclass="CbRO" id="cbCanvassers"
								model="@{canvasserList}" autodrop="true" tooltiptext="" readonly="true">
								<comboitem self="@{each=canvassers}"
									label="@{canvassers.canvasserName}" value="@{canvassers.id}" />
							</combobox>
						--></row>
						<row>
							<label
								value="${c:l('utilities.manualInput.label.visitDate')}" />
							<hbox>
								<datebox sclass="Db" id="dbVisit" width="158px" format="dd/MM/yyyy"/>
								<label value="(DD/MM/YYYY)" />
								<!--<listbox
									onSelect="dbVisit.setFormat(self.selectedItem.value)" mold="select"
									rows="1">
									<listitem label="Default" value="MM/dd/yyyy" />
									<listitem label="MM/dd/yyyy hh:mm a"
									value="MM/dd/yyyy hh:mm a" />
									<listitem label="MM/dd/yyyy hh:mm:ss"
									value="MM/dd/yyyy hh:mm:ss" />
									<listitem label="MM/dd/yyyy"
									value="MM/dd/yyyy" />
									<listitem label="MM-dd-yy" value="MM-dd-yy" />
									</listbox>-->
							</hbox>
						</row>
						<row>
							<label value="${c:l('utilities.manualInput.label.visitTime')}"/>
							<hbox>
							<combobox sclass="Db" id="cbHHDates" autodrop="true" width="40px"
								tooltiptext="" readonly="false" model="@{hhList}">
								<comboitem self="@{each=hhs}"
									label="@{hhs}" value="@{hhs}" />
							</combobox>
							<label value=" : " />
							<combobox sclass="Db" id="cbMMDates" autodrop="true" width="40px"
								tooltiptext="" readonly="false" model="@{mmAndSSList}">
								<comboitem self="@{each=mms}"

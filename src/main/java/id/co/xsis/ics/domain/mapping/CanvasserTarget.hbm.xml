<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="com.xsis.ics.domain.CanvasserTarget" table="CANVASSER_TARGET">
    	
    	<id name="id" type="long">
            <column name="CANVASSER_TARGET_ID" />
            <generator class="sequence">
				<param name="sequence">CANVASSER_TARGET_ID_S</param>
			</generator>
        </id>
       
        <property name="createdBy" type="long">
        	<column name="created_by"/>
        </property>
       
        <property name="creationDate" type="timestamp">
        	<column name="CREATION_DATE"/>
        </property>
        
        <property name="lastUpdatedBy" type="long">
        	<column name="LAST_UPDATED_BY"/>
        </property>
       
        <property name="lastUpdatedDate" type="timestamp">
        	<column name="LAST_UPDATED_DATE"/>
        </property>		      
        
        <property name="canvasserTargetDp" type="big_decimal">
            <column name="CANVASSER_TARGET_DP" precision="22" scale="0" />
        </property>
        
        <property name="effectiveDate" type="date">
            <column name="EFFECTIVE_DATE" length="7" />
        </property>
        
        <property name="ineffectiveDate" type="date">
            <column name="INEFFECTIVE_DATE" length="7" />
        </property>
        
        <property name="canvasserTargetSp" type="big_decimal">
            <column name="CANVASSER_TARGET_SP" precision="22" scale="0" />
        </property>
        
        <property name="canvasserTargetPv" type="big_decimal">
            <column name="CANVASSER_TARGET_PV" precision="22" scale="0" />
        </property>
        
        <property name="canvasserTargetWeek" type="long">
            <column name="CANVASSER_TARGET_WEEK" precision="2" scale="0" />
        </property>
        
        <property name="canvasserTargetYear" type="long">
            <column name="CANVASSER_TARGET_YEAR" precision="4" scale="0" />
        </property>
        
         <property name="canvasserTargetMonth" type="long">
        	<column name="CANVASSER_TARGET_MONTH" precision="2" scale="0"/>
        </property>
        
        <many-to-one name="canvasser" class="com.xsis.ics.domain.Canvasser" fetch="select">
            <column name="CANVASSER_ID" precision="22" scale="0" />
        </many-to-one>       
        
    </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xsis.ics.domain.DealerTarget" table="DEALER_TARGET">
		
		<id name="id" type="long">
			<column name="DEALER_TARGET_ID" />
			<generator class="sequence">
				<param name="sequence">DEALER_TARGET_ID_S</param>
			</generator>
		</id>
		
		<property name="createdBy" type="long">
        	<column name="created_by"/>
        </property>
       
        <property name="creationDate" type="date">
        	<column name="CREATION_DATE"/>
        </property>
        
        <property name="lastUpdatedBy" type="long">
        	<column name="LAST_UPDATED_BY"/>
        </property>
       
        <property name="lastUpdatedDate" type="date">
        	<column name="LAST_UPDATED_DATE"/>
        </property>	
		
		<property name="effectiveDate" type="date">
			<column name="EFFECTIVE_DATE" />
		</property> 
		
		<property name="ineffectiveDate" type="date">
			<column name="INEFFECTIVE_DATE" />
		</property>
		
		<property name="dealerTargetDp" type="long">
			<column name="DEALER_TARGET_DP" />
		</property> 
		
		<property name="dealerTargetPv" type="long">
			<column name="DEALER_TARGET_PV" />
		</property> 
		
		<property name="dealerTargetMonth" type="long">
			<column name="DEALER_TARGET_MONTH" />
		</property> 
		
		<property name="dealerTargetYear" type="long">
			<column name="DEALER_TARGET_YEAR" />
		</property> 
		
		<property name="dealerTargetSp" type="long">
			<column name="DEALER_TARGET_SP" />
		</property> 
		
		<many-to-one name="dealer" class="com.xsis.ics.domain.Dealer" fetch="select">
			<column name="DEALER_ID" />
		</many-to-one> 
		
	</class>
</hibernate-mapping>	
		
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xsis.security.model.SecDepo" table="depo">
		
		<id name="id" type="long">
            <column name="DEPO_ID" />
            <generator class="sequence">
				<param name="sequence">depo_id_s</param>
			</generator>
        </id>
        
        <property name="createdBy" type="long">
        	<column name="created_by"/>
        </property>
       
        <property name="creationDate" type="date">
        	<column name="CREATION_DATE"/>
        </property>
        
        <property name="lastUpdatedBy" type="long">
        	<column name="LAST_UPDATED_BY"/>
        </property>
       
        <property name="lastUpdatedDate" type="date">
        	<column name="LAST_UPDATED_DATE"/>
        </property>		
        
        <property name="depoName" type="string">
        	<column name="DEPO_NAME"/>
        </property>

        <property name="depoLongitude" type="big_decimal">
            <column name="depo_longitude" />
        </property>
        
        <property name="depoLatitude" type="big_decimal">
            <column name="depo_latitude" />
        </property>
        
        <property name="depoCode" type="string">
        	<column name="DEPO_CODE"/>
        </property>
        
        <many-to-one name="subRegion" class="com.xsis.security.model.SecSubRegion" fetch="select">
			<column name="SUBREGION_ID" />
		</many-to-one>
		
	</class>
</hibernate-mapping>

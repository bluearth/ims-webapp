<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xsis.ics.domain.Questionnaire" table="QUESTIONNAIRE">
		<id name="id" type="java.lang.Long">
            <column name="QUESTIONNAIRE_ID" />
            <generator class="sequence">
				<param name="sequence">QUESTIONNAIRE_ID_S</param>
			</generator>
        </id>
        <property name="createdBy" type="java.lang.Long">
        	<column name="created_by"/>
        </property>
        <property name="creationDate" type="java.util.Date">
        	<column name="creation_date"/>
        </property>
        <property name="lastUpdatedBy" type="java.lang.Long">
        	<column name="last_updated_by"/>
        </property>
        <property name="lastUpdatedDate" type="java.util.Date">
        	<column name="last_udated_date"/>
        </property>
        <property name="quesCode" type="string">
        	<column name="QUESTIONNAIRE_CODE"/>
        </property>
        <property name="quesEffectiveDate" type="java.util.Date">
        	<column name="EFFECTIVE_DATE"/>
        </property>
        <property name="quesIneffectiveDate" type="java.util.Date">
        	<column name="INEFFECTIVE_DATE"/>
        </property>
        <property name="quesDescription" type="string">
        	<column name="QUESTIONNAIRE_DESCRIPTION"/>
        </property>
        <property name="quesReferrenceTo" type="string">
        	<column name="QUESTIONNAIRE_REFERENCE_TO"/>
        </property>
        <property name="quesReferrenceId" type="java.lang.Long">
        	<column name="QUESTIONNAIRE_REFERENCE_ID"/>
        </property>
    </class>
</hibernate-mapping>

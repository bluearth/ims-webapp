<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.xsis.ics.domain.Parameter" table="ICS_PARAMETERS">
    	
    	<id name="id" type="long">
            <column name="PARAMETER_ID" />
            <generator class="sequence">
				<param name="sequence">PARAMETER_ID_S</param>
			</generator>
        </id>
       
        <property name="createdBy" type="long">
        	<column name="CREATED_BY"/>
        </property>
        
        <property name="creationDate" type="date">
        	<column name="CREATION_DATE"/>
        </property>
       
        <property name="lastUpdatedBy" type="long">
        	<column name="LAST_UPDATED_BY"/>
        </property>
       
        <property name="lastUpdatedDate" type="date">
        	<column name="LAST_UPDATED_DATE"/>
        </property>
        
        <property name="paramKey" type="string">
            <column name="PARAMETER_KEY" />
        </property>
        <property name="paramValue" type="string">
            <column name="PARAMETER_VALUE" />
        </property>
    </class>
</hibernate-mapping>
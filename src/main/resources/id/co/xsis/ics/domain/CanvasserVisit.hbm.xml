        </property>
        
        <property name="answer10" type="string">
            <column name="ANSWER_10" />
        </property>
        
        <property name="transactionKPI" type="string">
            <column name="TRANSACTION_KPI_FLAG" />
        </property>
        
		<property name="systemOrigin" type="string">
            <column name="SYSTEM_ORIGIN" />
        </property>
        
        
        <many-to-one name="outlet" class="com.xsis.ics.domain.Outlet" fetch="select">
			<column name="OUTLET_ID" precision="22" scale="0" not-null="true" />
		</many-to-one>
		
		<many-to-one name="canvasser" class="com.xsis.ics.domain.Canvasser" fetch="select">
			<column name="CANVASSER_ID" />
		</many-to-one>
		
		<set name="surveyResults" inverse="true" table="SURVEY_RESULT" fetch="select" cascade="all,delete-orphan">
			<key>
				<column name="CANVASSER_VISIT_ID" />
			</key>
			<one-to-many class="com.xsis.ics.domain.SurveyResult"/>
		</set>
        
	</class>
	
</hibernate-mapping>

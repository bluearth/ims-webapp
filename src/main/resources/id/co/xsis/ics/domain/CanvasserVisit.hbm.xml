        <property name="questionnaire9" type="string">
            <column name="QUESTIONNAIRE_9" />
        </property>
        
        <property name="questionnaire10" type="string">
            <column name="QUESTIONNAIRE_10" />
        </property>
        
        <property name="answer1" type="string">
            <column name="ANSWER_1" />
        </property>
        
        <property name="answer2" type="string">
            <column name="ANSWER_2" />
        </property>
        
        <property name="answer3" type="string">
            <column name="ANSWER_3" />
        </property>
        
        <property name="answer4" type="string">
            <column name="ANSWER_4" />
        </property>
        
        <property name="answer5" type="string">
